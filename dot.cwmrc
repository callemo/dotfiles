# dot.cwmrc
borderwidth 4
color       activeborder black
color       inactiveborder gray
fontname    "-b&h-lucida-bold-r-*-*-14-140-*-*-*-*-iso10646-*"
snapdist    4

autogroup   2 acme
autogroup   3 firefox

command     Acme    "/bin/sh -lc 'exec 9start -f $PLAN9/font/lucm/unicode.9.font'"
command     Firefox firefox

ignore xclock
ignore xconsole
ignore xload
