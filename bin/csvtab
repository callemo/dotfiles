#!/usr/bin/env python3
"""Convert csv to tab-delimited data"""
import csv
import fileinput
import os
import sys

try:
    reader = csv.reader(fileinput.input())
    for row in reader:
        print("\t".join(row))
except BrokenPipeError:
    os.dup2(os.open(os.devnull, os.O_WRONLY), sys.stdout.fileno())
